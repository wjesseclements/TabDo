{
  "manifest_version": 3,
  "name": "TabDo",
  "version": "1.0.0",
  "description": "A minimalist to-do list that transforms your new tab page into a productivity tool",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "author": "TabDo Team",
  "homepage_url": "https://github.com/wjesseclements/TabDo",
  "browser_specific_settings": {
    "gecko": {
      "id": "tabdo@example.com",
      "strict_min_version": "109.0"
    }
  }
}